{
  "name": "ts-lexgen",
  "version": "0.0.1",
  "description": "Lexer generator that outputs fully typed typescript.",
  "main": "main.js",
  "scripts": {
    "build": "tsc --build",
    "run": "node dist/main.js",
    "bootstrap_stage1": "tsc --build bootstrap/stage1 && node bootstrap/stage1/dist/bootstrap/stage1/stage1.js",
    "bootstrap_stage2": "tsc --build bootstrap/stage2 && node bootstrap/stage2/dist/bootstrap/stage2/Stage2.js",
    "bootstrap_stage3": "tsc --build bootstrap/stage3 && node bootstrap/stage3/dist/bootstrap/stage3/Stage3.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.6"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.6",
    "@types/node": "^15.0.1",
    "typescript": "^4.2.4"
  }
}
